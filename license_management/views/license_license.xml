<?xml version="1.0" encoding="utf-8"?>
<odoo>

  <record id="license_view_tree" model="ir.ui.view">
    <field name="name">license_management.license_view_tree</field>
    <field name="model">license.license</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="value"/>
        <field name="type_id"/>
      </tree>
    </field>
  </record>

  <record id="license_open_view_tree" model="ir.actions.act_window">
    <field name="name">Licenses</field>
    <field name="res_model">license.license</field>
    <field name="view_mode">tree,form</field>
  </record>

  <menuitem name="License Management" id="license_management.menu_root" web_icon="license_management,static/description/icon.png" />

  <menuitem name="Licenses" id="license_management.menu_licenses" parent="license_management.menu_root"/>
  <menuitem name="Licenses" id="license_management.menu_license" parent="license_management.menu_licenses" action="license_management.license_open_view_tree"/>

</odoo>